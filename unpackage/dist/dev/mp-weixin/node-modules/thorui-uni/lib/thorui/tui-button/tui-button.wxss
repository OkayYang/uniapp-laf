/* button start*/
.tui-btn.data-v-3da687df {
	width: 100%;
	position: relative;
	border: 0 !important;
	border-radius: 6rpx;
	padding-left: 0;
	padding-right: 0;
	overflow: visible;
}
.tui-btn.data-v-3da687df::after {
	content: '';
	position: absolute;
	width: 200%;
	height: 200%;
	-webkit-transform-origin: 0 0;
	        transform-origin: 0 0;
	-webkit-transform: scale(0.5, 0.5) translateZ(0);
	        transform: scale(0.5, 0.5) translateZ(0);
	box-sizing: border-box;
	left: 0;
	top: 0;
	border-radius: 12rpx;
	border: 0;
}
